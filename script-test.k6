import http from 'k6/http';
import { check, sleep } from 'k6';

export let options = {
  stages: [
    { duration: '10s', target: 10 } // 10 threads simultâneas por 10 segundos
  ],
};

export default function () {
  // Altere a URL para a sua API REST
  let url = 'http://localhost:9000/bulk/insert10000';


  let params = {
    headers: {
      'Content-Type': 'text/plain',
    },
  };

  // Faça uma chamada à API REST
  let response = http.post(url, params);

  // Verifique se a resposta foi bem-sucedida
  check(response, {
    'status is 201': (r) => r.status === 201,
  });

  // Aguarde um curto período entre as chamadas (opcional)
  //sleep(1);
}